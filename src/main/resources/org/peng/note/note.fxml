<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.peng.note.NoteController">
    <children>
        <ButtonBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="600.0">
            <buttons>
                <Button fx:id="b1" mnemonicParsing="false" prefHeight="24.0" prefWidth="81.0" text="Button"/>
                <Button fx:id="b2" mnemonicParsing="false" text="关闭"/>
            </buttons>
        </ButtonBar>
        <TextArea fx:id="text1" layoutY="40.0" prefHeight="360.0" prefWidth="600.0"/>
    </children>
</AnchorPane>
